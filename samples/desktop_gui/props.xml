<properties xmlns="http://www.ast.com/adk/properties">
    <title>Sample properties</title>
    <description>
        Sample of properties sheet
    </description>
    <item name="Item1" type="integer" maxValue="30" minValue="20">
        <description>
            Optional long description.
        </description>
        <value>25</value>
    </item>
    <item name="Item2" dispName="Item 2" type="float" units="mA*h">120.5</item>
    <category name="sample" dispName="Sample category">
        <description>
            Place long description here. It is automatically   
            re-formatted.
            
            Empty line before new paragraph.
        </description>
        <item name="Item3" type="string" maxLen="20">Test string</item>
        <item name="Item4" type="boolean">true</item>
        <item name="Item5" type="boolean" value="true" />
    </category>
</properties>
